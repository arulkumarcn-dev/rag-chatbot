{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenAI": {
    "ApiKey": "your-openai-api-key-here",
    "EmbeddingModel": "text-embedding-3-small",
    "ChatModel": "gpt-4"
  },
  "Gemini": {
    "ApiKey": "your-gemini-api-key-here",
    "Model": "gemini-pro"
  },
  "VectorStore": {
    "Type": "FAISS",
    "StoragePath": "./vectorstore",
    "Dimension": 1536
  },
  "DocumentStorage": {
    "Path": "./documents"
  }
}
